{
    "name": "change-stepper",
    "displayName": "Change Stepper",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "change-stepper.helloWorld",
                "title": "Hello VS Code"
            }
        ],
        "keybindings": [
            {
                "command": "change-stepper.next-line",
                "key": "ctrl+alt+j",
                "mac": "cmd+alt+j"
            },
            {
                "command": "change-stepper.previous-line",
                "key": "ctrl+alt+k",
                "mac": "cmd+alt+k"
            },
            {
                "command": "change-stepper.next-token",
                "key": "ctrl+alt+l",
                "mac": "cmd+alt+l"
            },
            {
                "command": "change-stepper.previous-token",
                "key": "ctrl+alt+h",
                "mac": "cmd+alt+h"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.74.0",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "@vscode/test-electron": "^2.2.0",
        "eslint": "^8.28.0",
        "glob": "^8.0.3",
        "mocha": "^10.1.0",
        "typescript": "^4.9.3"
    }
}
